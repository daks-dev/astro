{
  "name": "@daks.dev/astro.sdk",
  "version": "0.1.7",
  "description": "DAKS.DEV Astro software development kit",
  "type": "module",
  "scripts": {
    "info": "astro info",
    "check": "astro check && svelte-check",
    "check:watch": "astro check --watch",
    "check:watch:svelte": "svelte-check --watch",
    "test": "vitest --ui",
    "run:prettier": "prettier --plugin=prettier-plugin-astro --plugin=prettier-plugin-svelte --ignore-path=../../.prettierignore",
    "lint": "pnpm run:prettier --check . && eslint .",
    "format": "pnpm run:prettier --write .",
    "package": "svelte-package && publint",
    "prepublishOnly": "pnpm check && pnpm package",
    "run:publish": "pnpm publish",
    "run:update": "pnpm up"
  },
  "devDependencies": {
    "@astrojs/check": "^0.2.1",
    "@astrojs/svelte": "^4.0.3",
    "@astrojs/tailwind": "^5.0.2",
    "@astrojs/ts-plugin": "^1.2.0",
    "@nanostores/persistent": "^0.9.1",
    "@nanostores/react": "^0.7.1",
    "@sveltejs/package": "^2.2.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/html-minifier-terser": "^7.0.1",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.32",
    "@types/react-dom": "^18.2.14",
    "@vitest/ui": "^0.34.6",
    "astro": "^3.3.4",
    "html-minifier-terser": "^7.2.0",
    "nanostores": "^0.9.4",
    "publint": "^0.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vanilla-lazyload": "^17.8.5",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "astro": "^3.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "svelte": "^4.2.2",
    "tailwindcss": "^3.3.5"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "svelte": "./dist/index.js"
    },
    "./astro": {
      "types": "./dist/astro/index.d.ts",
      "import": "./dist/astro/index.js"
    },
    "./astro/*": {
      "types": "./dist/astro/*/index.d.ts",
      "import": "./dist/astro/*/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js"
    },
    "./react/*": {
      "types": "./dist/react/*/index.d.ts",
      "import": "./dist/react/*/index.js"
    },
    "./svelte": {
      "types": "./dist/svelte/index.d.ts",
      "import": "./dist/svelte/index.js"
    },
    "./svelte/*": {
      "types": "./dist/svelte/*/index.d.ts",
      "import": "./dist/svelte/*/index.js"
    },
    "./stores": {
      "types": "./dist/app/stores/index.d.ts",
      "import": "./dist/app/stores/index.js"
    },
    "./stores/*": {
      "types": "./dist/app/stores/*.d.ts",
      "import": "./dist/app/stores/*.js"
    },
    "./tailwind": {
      "types": "./dist/tailwind/index.d.ts",
      "import": "./dist/tailwind/index.js",
      "require": "./dist/tailwind/index.js"
    },
    "./tailwind/*": {
      "types": "./dist/tailwind/*.d.ts",
      "import": "./dist/tailwind/*.js",
      "require": "./dist/tailwind/*.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.js"
    },
    "./icons/*": "./dist/assets/icons/*",
    "./images/*": "./dist/assets/images/*",
    "./styles/*": "./dist/assets/styles/*"
  },
  "typesVersions": {
    ">4.0": {
      "tailwind": [
        "./dist/tailwind/index.d.ts"
      ],
      "tailwind/*": [
        "./dist/tailwind/*.d.ts"
      ]
    }
  },
  "types": "dist/index.d.ts",
  "svelte": "./dist/index.js",
  "files": [
    "assets",
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "engines": {
    "node": ">=16.20.2",
    "pnpm": "^8.8.0"
  },
  "browserslist": [
    "last 2 version",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/daks-dev/astro",
    "directory": "packages/sdk"
  },
  "author": {
    "name": "DVA",
    "email": "mail@daks.dev",
    "url": "https://daks.dev"
  },
  "bugs": "https://github.com/daks-dev/astro/issues",
  "homepage": "https://daks.dev/",
  "keywords": [
    "astro",
    "astro-component",
    "astrojs",
    "react",
    "svelte",
    "sveltejs",
    "tailwind",
    "tailwindcss",
    "vite",
    "withastro"
  ]
}
