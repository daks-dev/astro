{
  "name": "@daks.dev/sdk",
  "version": "0.1.2",
  "description": "DAKS.DEV software development kit",
  "type": "module",
  "scripts": {
    "check": "astro check && svelte-check --tsconfig ./tsconfig.svelte.json",
    "check:watch": "astro check --watch",
    "check:watch:svelte": "svelte-check --tsconfig ./tsconfig.svelte.json --watch",
    "package": "pnpm check && publint",
    "prepublishOnly": "pnpm package",
    "test": "vitest",
    "lint": "prettier --plugin=prettier-plugin-astro --check . && eslint .",
    "format": "prettier --plugin=prettier-plugin-astro --write .",
    "update": "pnpm up"
  },
  "devDependencies": {
    "@astrojs/check": "^0.2.0",
    "@astrojs/ts-plugin": "^1.1.3",
    "@nanostores/persistent": "^0.9.1",
    "@nanostores/react": "^0.7.1",
    "@types/node": "^20.7.1",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "astro": "^3.1.4",
    "canvas-confetti": "^1.6.1",
    "esm-env": "^1.0.0",
    "nanostores": "^0.9.3",
    "publint": "^0.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-imagetools": "^5.0.8",
    "vitest": "^0.34.5"
  },
  "peerDependencies": {
    "astro": "^3.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "svelte": "^4.2.1",
    "tailwindcss": "^3.3.3"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/utils/index.ts"
    },
    "./astro": "./src/astro/index.ts",
    "./react": "./src/react/index.ts",
    "./svelte": "./src/svelte/index.ts",
    "./tailwind": "./src/tailwind/index.ts",
    "./styles/*": "./src/styles/*"
  },
  "typesVersions": {
    ">4.0": {
      "tailwind": [
        "./src/tailwind/index.ts"
      ]
    }
  },
  "types": "src/index.d.ts",
  "files": [
    "src",
    "!src/**/*.test.*",
    "!src/**/*.spec.*"
  ],
  "engines": {
    "node": ">=16.20.2",
    "pnpm": "^8.8.0"
  },
  "browserslist": [
    "last 2 version",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/daks-dev/astro",
    "directory": "packages/sdk"
  },
  "author": {
    "name": "DVA",
    "email": "mail@daks.dev",
    "url": "https://daks.dev"
  },
  "bugs": "https://github.com/daks-dev/astro/issues",
  "homepage": "https://daks.dev/",
  "keywords": [
    "astro",
    "astro-component",
    "astrojs",
    "react",
    "svelte",
    "sveltejs",
    "tailwind",
    "tailwindcss",
    "vite",
    "withastro"
  ]
}
